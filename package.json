{
	"name": "sbundler",
	"version": "1.1.0",
	"description": "A bundler for SB projects.",
	"main": "dist/index.js",
	"scripts": {
		"start": "node --unhandled-rejections=strict -r ts-node/register bin/index.js",
		"build": "tsc",
		"build:check": "tsc --noEmit"
	},
	"files": [
		"dist"
	],
	"bin": {
		"sbundler": "./bin/index.js"
	},
	"preferGlobal": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/romdotdog/sbundler.git"
	},
	"author": "romdotdog",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/romdotdog/sbundler/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"homepage": "https://github.com/romdotdog/sbundler#readme",
	"devDependencies": {
		"@types/node": "^14.14.37",
		"@typescript-eslint/eslint-plugin": "^4.21.0",
		"@typescript-eslint/parser": "^4.21.0",
		"eslint": "^7.23.0",
		"eslint-config-prettier": "^8.1.0",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.4"
	},
	"dependencies": {
		"cac": "^6.7.2",
		"escalade": "^3.1.1",
		"glob": "^7.1.6",
		"glob-promise": "^4.1.0"
	}
}
